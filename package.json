    "@types/lodash": "^4.14.201",
    "@types/node": "^17.0.41",
    "@types/qrcode.react": "^1.0.5",
    "@types/react-router": "^5.1.20",
    "@types/scryptsy": "^2.0.0",
    "@types/secp256k1": "^3.5.0",
    "@types/yargs": "^17.0.31",
    "assert": "^2.1.0",
    "axios": "^1.6.1",
